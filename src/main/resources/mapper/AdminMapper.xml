<mapper namespace="com.wechatweb.demo.mapper.AdminMapper">
    <select id="login" parameterType="com.wechatweb.demo.entity.admin" resultType="com.wechatweb.demo.entity.admin">
        select * FROM admin WHERE code = '${code}'  and password = '${password}'
